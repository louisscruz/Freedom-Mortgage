{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "typings/main.d.ts",
    "typings/main"
  ],
  "filesGlob": [
    "./src/**/*.ts",
    "./test/**/*.ts",
    "!./node_modules/**/*.ts",
    "src/custom-typings.d.ts",
    "typings/browser.d.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  },
  "files": [
    "./src/app/about/about.spec.ts",
    "./src/app/about/about.ts",
    "./src/app/app.e2e.ts",
    "./src/app/app.spec.ts",
    "./src/app/app.ts",
    "./src/app/apply/apply.ts",
    "./src/app/contact/contact.ts",
    "./src/app/directives/router-active.ts",
    "./src/app/home/directives/x-large.spec.ts",
    "./src/app/home/directives/x-large.ts",
    "./src/app/home/home.e2e.ts",
    "./src/app/home/home.spec.ts",
    "./src/app/home/home.ts",
    "./src/app/home/services/title.spec.ts",
    "./src/app/home/services/title.ts",
    "./src/app/navbar/navbar.spec.ts",
    "./src/app/navbar/navbar.ts",
    "./src/custom-typings.d.ts",
    "./src/main.ts",
    "./src/polyfills.ts",
    "src/custom-typings.d.ts",
    "typings/browser.d.ts"
  ]
}
